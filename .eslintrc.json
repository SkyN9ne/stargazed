{
	"extends": [
		"@abhijithvijayan/eslint-config",
		"@abhijithvijayan/eslint-config/node"
	],
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"camelcase": "off",
		"consistent-return": "warn",
		"node/shebang": "off",
		"node/no-unsupported-features/es-syntax": ["error", {
			"ignores": ["modules"]
		}],
		"no-console": "off",
		"no-extend-native": "off",
		"no-process-exit": "off"
	}
}
